@{
    ViewBag.Title = "Консоль";
}

<div class="row card">
    <button class="tab-button" onclick="openTab(this, 'general-tab'); initGeneral();">@ViewBag.Title</button>
</div>

<div id="general-tab" class="hidden tab card">
    <div class="row">
        <div class="col s12 m12 l12">
            <div id="general-div" class="pading-10"></div>
        </div>
    </div>
</div>

@section Scripts {
    @{
        <environment names="Development">
            <script src="~/scripts/console/index.js"></script>
        </environment>
        <environment names="Staging,Production">
            <script src="~/scripts/console/index.min.js"></script>
        </environment>
    }
}