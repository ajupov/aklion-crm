@{
    ViewBag.Title = "Аудит действий";
}

<div class="row card">
    <button class="tab-button" onclick="openTab(this, 'audit-logs-tab'); initAuditLogsTable();">@ViewBag.Title</button>
</div>

<div id="audit-logs-tab" class="hidden tab card">
    <div class="row">
        <div class="col s12 m12 l12">
            <div class="horisontal-scrollable pading-10">
                <table id="audit-logs-table"></table>
                <div id="audit-logs-table-pagination"></div>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    @{
        <environment names="Development">
            <script src="~/scripts/administration/auditLog/index.js"></script>
        </environment>
        <environment names="Staging,Production">
            <script src="~/scripts/administration/auditLog/index.min.js"></script>
        </environment>
    }
}